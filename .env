# File Downloader Configuration
DOWNLOAD_DIRECTORY=/mnt/volume_ams3_01
CONCURRENT_DOWNLOADS=3
TIMEOUT_MS=30000
RETRY_ATTEMPTS=3
RETRY_DELAY_MS=5000

# Download URLs (one per line, add your URLs here)
# DOWNLOAD_URLS=https://example.com/file1.zip,https://example.com/file2.pdf

# Optional: Log level (info, warn, error)
LOG_LEVEL=info

# Unzip Configuration
AUTO_UNZIP=true
UNZIP_SOURCE_DIRECTORY=/mnt/volume_ams3_01
UNZIP_DESTINATION_DIRECTORY=/mnt/unzippedimages
DELETE_AFTER_UNZIP=false
CONCURRENT_EXTRACTIONS=2
